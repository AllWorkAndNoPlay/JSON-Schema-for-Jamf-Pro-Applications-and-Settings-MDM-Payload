                {
                    "title": "Configured",
                    "type": "boolean"
                }
            ]
        },
        "enablehidcontrol": {
            "title": "Enable HID Control of USB Audio Devices",
            "anyOf": [
                {
                    "type": "null",
                    "title": "Not Configured"
                },
                {
                    "title": "Configured",
                    "type": "boolean"
                }
            ]
