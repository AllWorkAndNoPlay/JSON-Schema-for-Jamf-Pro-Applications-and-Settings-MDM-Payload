            "title": "Require Facebook Sign in on Application Launch",
            "anyOf": [
                {
                    "type": "null",
                    "title": "Not Configured"
                },
                {
                    "title": "Configured",
                    "type": "boolean"
                }
            ]
        },
        "enableembedbrowserforsso": {
            "title": "Launch SSO Login in App",
            "description": "(enableembedbrowserforsso)-Launches the SSO Sign in in App rather than the user's default browser",
            "anyOf": [
                {
                    "type": "null",
                    "title": "Not Configured"
                },
                {
                    "title": "Configured",
                    "type": "boolean"
                }
            ]
        },
        "setwebdomain": {
            "title": "Sign in Web Domain",
            "anyOf": [
                {
                    "type": "null",
                    "title": "Not Configured"
                },
                {
                    "title": "Configured",
                    "type": "string",
                    "options": {
                        "inputAttributes": {
                            "placeholder": "https://zoom.us"
                        }
                    }
                }
            ]
        },
        "LastLoginType": {
            "title": "Remember the last Login Type Used",
